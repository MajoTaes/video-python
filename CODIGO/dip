from abc import ABC, abstractclassmethod

class VerificadorOrtografico(ABC):
    @abstractclassmethod
    def verificar_palabra(self,palabra):
        pass
    
class Diccionario(VerificadorOrtografico):
    def verificar_palabra(self, palabra):
        pass

class ServicioOnline(VerificadorOrtografico):
    def verificar_palabra(self,palabra):
        pass
    
